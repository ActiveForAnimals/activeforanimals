{#
/**
 * @file
 * An import form page.
 *
 */
#}
<form{{ attributes }}>
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}
  <div class="section transparent">
    <div class="title">
      {{ 'Import'|t }}
    </div>
    <div class="column-wrapper">
      <div class="column-8-15 first-column">
        {{ form.parent }}
        {{ form.field_file_csv }}
      </div>
      <div class="column-7-15 last-column">
        {{ form.timezone_notice }}
      </div>
    </div>
    {{ form.instructions }}
    <div class="footer">
      {{ form.actions.submit }}
    </div>
  </div>
</form>

{#
/**
 * @file
 * An export page.
 *
 */
#}
<div class="section">
  <div class="title">
    {{ 'Export'|t }}
  </div>
  <div class="content">
    <div class="column-wrapper">
      <div class="column-11-15 first-column">
        <div class="section">
          <div class="column-wrapper">
            <div class="column-7-15 first-column">
              {{ content.filter }}
            </div>
            <div class="column-8-15 last-column">
            </div>
          </div>
        </div>
        <div class="title">
          {{ 'File'|t }}
        </div>
        {{ content.field_file_csv }}
      </div>
      <div class="column-4-15 last-column">
        {% if content.links is not empty %}
        <div class="links">
          <div class="title">
            {{ 'Operations'|t }}
          </div>
          <div class="content">
            <ul>
              {% for element in content.links %}
              {% if element|render %}
              <li>{{ element }}</li>
              {% endif %}
              {% endfor %}
            </ul>
          </div>
        </div>
        {% endif %}
      </div>
    </div>
  </div>
  <div class="footer">
    {{ help_button }}
  </div>
</div>

{#
/**
 * @file
 * The user login form.
 *
 */
#}
<form{{ attributes }}>
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}
  <div class="column-wrapper">
    <div class="column-5-15 no-spacing">
      {{ form.name }}
    </div>
    <div class="column-5-15 no-spacing">
      {{ form.pass }}
    </div>
    <div class="column-5-15 no-spacing">
      {{ form.actions.submit }}
    </div>
  </div>
</form>
